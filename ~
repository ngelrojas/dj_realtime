const url = 'http://127.0.0.1:8000/api/v1/notes/';

export const fetchNotes = async () => {
  return fetch(url, {})
    .then(res => res.json())
    .then(data => {
          return data
        });
}

export const fetchNote = (id) => {
  return fetch(`${url + id}`, {})
    .then(res => res.json())
    .then(data => {
          return data;
        });
}


